//Criar um programa que calcula a média
//das notas entre os alunos e envia
//mensagem do cálculo da média.

const alunos = [
    {
        nome: 'Gabriel',
        nota: 9.8
    },
    {
        nome: 'Jão',
        nota: 10
    },
    {
        nome: 'Fulano',
        nota: 2
    }
]

const nomesDeAlunos = ['Gabriel', 'Jão', 'Fulano']
 console.log(alunos)

const media = (alunos[0].nota + alunos[1] + alunos[2].nota)/3


//Se a média for maior que 5, parabenizar a turma.

if(media > 5) {
    console.log(`A media foi de ${media}. Parabéns!`)
} else{
    console.log('A média foi menor que 5')
}
